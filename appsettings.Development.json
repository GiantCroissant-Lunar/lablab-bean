{
  "Akka": {
    "Persistence": {
      "ConnectionString": "Data Source=avatars.db",
      "ProviderName": "Microsoft.Data.Sqlite"
    }
  },
  "KernelMemory": {
    "Embedding": {
      "MaxTokens": 8191,
      "ModelName": "text-embedding-3-small",
      "Provider": "OpenAI"
    },
    "Storage": {
      "CollectionName": "game_memories",
      "ConnectionString": "http://localhost:6333",
      "Provider": "Qdrant"
    },
    "TextGeneration": {
      "MaxTokens": 4096,
      "ModelName": "gpt-4o",
      "Provider": "OpenAI"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "LablabBean.AI": "Debug",
      "Microsoft": "Warning"
    }
  },
  "MediaPlayer": {
    "BufferSize": 5,
    "DefaultVolume": 0.75,
    "MaxFrameRate": 30,
    "PreferredRenderer": "Auto",
    "SupportedFormats": [
      ".mp4",
      ".mkv",
      ".avi",
      ".webm",
      ".mov",
      ".mp3",
      ".wav",
      ".flac",
      ".ogg",
      ".m4a"
    ]
  },
  "OpenAI": {
    "ApiKey": "YOUR_OPENAI_API_KEY_HERE",
    "EmbeddingModelId": "text-embedding-3-small",
    "ModelId": "gpt-4o"
  },
  "Plugins": {
    "PreferredRenderer": "rendering-terminal",
    "PreferredUI": "ui-terminal",
    "SearchPaths": [
      "plugins",
      "plugins/MediaPlayer"
    ],
    "Skip": [],
    "Only": [],
    "StrictCapabilityMode": true
  }
}
