{
  "$id": "https://lablab-bean.local/schemas/qdrant/search-response.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "additionalProperties": false,
  "description": "Response wrapper for Qdrant vector search API containing result array",
  "properties": {
    "result": {
      "description": "Array of search results ordered by similarity score (descending)",
      "items": {
        "additionalProperties": false,
        "properties": {
          "id": {
            "description": "Point identifier (can be string or integer in Qdrant API)",
            "oneOf": [
              {
                "type": "string"
              },
              {
                "type": "integer"
              }
            ]
          },
          "payload": {
            "additionalProperties": true,
            "description": "Dynamic payload data (omitted if with_payload=false)",
            "type": [
              "object",
              "null"
            ]
          },
          "score": {
            "description": "Similarity score (0.0 to 1.0, higher is more similar)",
            "maximum": 1.0,
            "minimum": 0.0,
            "type": "number"
          },
          "vector": {
            "description": "Vector embeddings (omitted if with_vector=false)",
            "oneOf": [
              {
                "items": {
                  "type": "number"
                },
                "type": "array"
              },
              {
                "type": "null"
              }
            ]
          }
        },
        "required": [
          "id",
          "score"
        ],
        "type": "object"
      },
      "type": "array"
    },
    "status": {
      "description": "Response status indicator",
      "enum": [
        "ok",
        "error"
      ],
      "type": "string"
    },
    "time": {
      "description": "Execution time in seconds",
      "minimum": 0,
      "type": [
        "number",
        "null"
      ]
    }
  },
  "required": [
    "result",
    "status"
  ],
  "title": "QdrantSearchResponse",
  "type": "object"
}
